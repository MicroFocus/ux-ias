{
  "name": "@microfocus/ux-ias",
  "version": "1.1.2",
  "description": "UX library for Micro Focus Identity, Access, and Security products",
  "engines": {
    "node": ">=6.2",
    "npm": ">=3.9"
  },
  "scripts": {
    "prepublishOnly": "gulp clean && gulp build:production",
    "build": "npm run prepublishOnly & npm run build:docs",
